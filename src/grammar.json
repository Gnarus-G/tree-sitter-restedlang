{
  "name": "restedlang",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "item"
      }
    },
    "item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "SYMBOL",
          "name": "attribute"
        },
        {
          "type": "SYMBOL",
          "name": "request"
        },
        {
          "type": "SYMBOL",
          "name": "line_comment"
        }
      ]
    },
    "request": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "request_method"
        },
        {
          "type": "SYMBOL",
          "name": "endpoint"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "block"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "request_method": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "get"
        },
        {
          "type": "STRING",
          "value": "post"
        },
        {
          "type": "STRING",
          "value": "put"
        },
        {
          "type": "STRING",
          "value": "patch"
        },
        {
          "type": "STRING",
          "value": "delete"
        }
      ]
    },
    "endpoint": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "SYMBOL",
          "name": "url_literal"
        },
        {
          "type": "SYMBOL",
          "name": "pathname_literal"
        }
      ]
    },
    "url_literal": {
      "type": "PATTERN",
      "value": "https?:.*"
    },
    "pathname_literal": {
      "type": "PATTERN",
      "value": "\\/.*"
    },
    "_declaration": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "constant_declaration"
        },
        {
          "type": "SYMBOL",
          "name": "variable_declaration"
        }
      ]
    },
    "constant_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "set"
        },
        {
          "type": "SYMBOL",
          "name": "constant_identifier"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "variable_declaration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "let"
        },
        {
          "type": "SYMBOL",
          "name": "identifer"
        },
        {
          "type": "STRING",
          "value": "="
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "attribute": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "@"
        },
        {
          "type": "SYMBOL",
          "name": "identifer"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "parameter_list"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "call"
        },
        {
          "type": "SYMBOL",
          "name": "identifer"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "STRING",
          "value": "null"
        }
      ]
    },
    "identifer": {
      "type": "PATTERN",
      "value": "[a-z_][_\\w]*"
    },
    "constant_identifier": {
      "type": "PATTERN",
      "value": "[A-Z_][A-Z_\\d]*"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d*)?"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"]*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "call": {
      "type": "PREC",
      "value": 1,
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_expression"
          },
          {
            "type": "SYMBOL",
            "name": "parameter_list"
          }
        ]
      }
    },
    "parameter_list": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "_expression"
                },
                {
                  "type": "SYMBOL",
                  "name": "line_comment"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "block": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_statement"
          }
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "_statement": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "header_statement"
        },
        {
          "type": "SYMBOL",
          "name": "body_statement"
        },
        {
          "type": "SYMBOL",
          "name": "line_comment"
        }
      ]
    },
    "header_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "header"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "body_statement": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "body"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        },
        {
          "type": "STRING",
          "value": "\n"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
